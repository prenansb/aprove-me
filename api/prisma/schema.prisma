generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model assignor {
  id       String    @id @default(uuid())
  document String
  email    String
  phone    String
  name     String
  payables payable[]
}

model payable {
  id           String   @id @default(uuid())
  value        Float
  emissionDate DateTime
  assignorId   String
  assignor     assignor @relation(fields: [assignorId], references: [id])
}
